{
  "name": "shoutout",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "sass": "node-sass --output-style compressed ./src/scss -o ./public/css",
    "sass:watch": "node-sass --output-style expanded -w ./src/scss/ -o ./public/css/",
    "server": "node ./src/server.js",
    "server:watch": "nodemon ./src/server.js",
    "start": "npm-run-all --sequential sass server",
    "dev": "npm-run-all sass --parallel **:watch"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "mongodb": "^3.2.3",
    "mongoose": "^5.5.4",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "nodemon": "^1.18.11"
  }
}
